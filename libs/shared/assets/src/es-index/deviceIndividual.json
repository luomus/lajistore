{
  "settings": {
    "number_of_shards": 3,
    "index.mapping.ignore_malformed": true,
    "index.max_result_window": 1000000,
    "index.mapping.total_fields.limit": 10000,
    "index.mapping.nested_objects.limit": 100000
  },
  "mappings": {
    "properties": {
      "@context": {
        "type": "keyword"
      },
      "id": {
        "type": "keyword"
      },
      "@type": {
        "type": "keyword"
      },
      "attached": {
        "type": "date"
      },
      "deviceID": {
        "type": "keyword",
        "ignore_above": 200
      },
      "individualID": {
        "type": "keyword",
        "ignore_above": 200
      },
      "removed": {
        "type": "date"
      },
      "_meta": {
        "properties": {
          "created": {
            "type": "date"
          },
          "edited": {
            "type": "date"
          }
        }
      }
    }
  }
}