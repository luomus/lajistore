{
  "settings": {
    "number_of_shards": 3,
    "index.mapping.ignore_malformed": true,
    "index.max_result_window": 1000000,
    "index.mapping.total_fields.limit": 10000,
    "index.mapping.nested_objects.limit": 100000,
    "index.max_ngram_diff": 4,
    "analysis": {
      "tokenizer": {
        "autocomplete": {
          "type": "ngram",
          "min_gram": 1,
          "max_gram": 4
        }
      },
      "normalizer": {
        "useLowercase": {
          "type": "custom",
          "filter": [
            "lowercase"
          ]
        }
      },
      "analyzer": {
        "autocomplete": {
          "type": "custom",
          "tokenizer": "autocomplete",
          "filter": [
            "lowercase"
          ]
        },
        "autocomplete_search": {
          "tokenizer": "lowercase"
        }
      }
    }
  },
  "mappings": {
    "properties": {
      "@context": {
        "type": "keyword"
      },
      "id": {
        "type": "keyword",
        "normalizer": "useLowercase",
        "fields": {
          "autocomplete": {
            "type": "text",
            "analyzer": "autocomplete",
            "search_analyzer": "autocomplete_search"
          }
        }
      },
      "@type": {
        "type": "keyword"
      },
      "agent": {
        "type": "keyword",
        "ignore_above": 200,
        "normalizer": "useLowercase",
        "fields": {
          "autocomplete": {
            "type": "text",
            "analyzer": "autocomplete",
            "search_analyzer": "autocomplete_search"
          }
        }
      },
      "amount": {
        "type": "keyword",
        "ignore_above": 200,
        "normalizer": "useLowercase",
        "fields": {
          "autocomplete": {
            "type": "text",
            "analyzer": "autocomplete",
            "search_analyzer": "autocomplete_search"
          }
        }
      },
      "amountEstimationUnit": {
        "type": "keyword",
        "ignore_above": 200,
        "normalizer": "useLowercase",
        "fields": {
          "autocomplete": {
            "type": "text",
            "analyzer": "autocomplete",
            "search_analyzer": "autocomplete_search"
          }
        }
      },
      "branchID": {
        "type": "keyword",
        "ignore_above": 200,
        "normalizer": "useLowercase",
        "fields": {
          "autocomplete": {
            "type": "text",
            "analyzer": "autocomplete",
            "search_analyzer": "autocomplete_search"
          }
        }
      },
      "date": {
        "type": "date"
      },
      "eRH": {
        "type": "keyword",
        "ignore_above": 200,
        "normalizer": "useLowercase",
        "fields": {
          "autocomplete": {
            "type": "text",
            "analyzer": "autocomplete",
            "search_analyzer": "autocomplete_search"
          }
        }
      },
      "estimatedSeedQuantity": {
        "type": "integer"
      },
      "eventType": {
        "type": "keyword",
        "ignore_above": 200
      },
      "germinationConditions": {
        "type": "keyword",
        "ignore_above": 200,
        "normalizer": "useLowercase",
        "fields": {
          "autocomplete": {
            "type": "text",
            "analyzer": "autocomplete",
            "search_analyzer": "autocomplete_search"
          }
        }
      },
      "germinationPercentage": {
        "type": "integer"
      },
      "germinationRate": {
        "type": "float"
      },
      "lifeStage": {
        "type": "keyword",
        "ignore_above": 200
      },
      "notes": {
        "type": "keyword",
        "ignore_above": 200,
        "normalizer": "useLowercase",
        "fields": {
          "autocomplete": {
            "type": "text",
            "analyzer": "autocomplete",
            "search_analyzer": "autocomplete_search"
          }
        }
      },
      "numberOfSeedsEmpty": {
        "type": "integer"
      },
      "numberOfSeedsFull": {
        "type": "integer"
      },
      "numberOfSeedsInfested": {
        "type": "integer"
      },
      "numberOfSeedsMouldy": {
        "type": "integer"
      },
      "numberOfSeedsPartFull": {
        "type": "integer"
      },
      "numberOfSeedsTested": {
        "type": "integer"
      },
      "percentageSeedsFull": {
        "type": "integer"
      },
      "seedsExchangedInstitution": {
        "type": "keyword",
        "ignore_above": 200,
        "normalizer": "useLowercase",
        "fields": {
          "autocomplete": {
            "type": "text",
            "analyzer": "autocomplete",
            "search_analyzer": "autocomplete_search"
          }
        }
      },
      "testPassed": {
        "type": "boolean"
      },
      "viabilityPercentage": {
        "type": "integer"
      },
      "_meta": {
        "properties": {
          "created": {
            "type": "date"
          },
          "edited": {
            "type": "date"
          }
        }
      }
    }
  }
}