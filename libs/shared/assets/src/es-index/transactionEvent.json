{
  "settings": {
    "number_of_shards": 3,
    "index.mapping.ignore_malformed": true,
    "index.max_result_window": 1000000,
    "index.mapping.total_fields.limit": 10000,
    "index.mapping.nested_objects.limit": 100000,
    "analysis": {
      "filter": {
        "autocomplete_filter": {
          "type": "edge_ngram",
          "min_gram": 1,
          "max_gram": 20
        }
      },
      "normalizer": {
        "useLowercase": {
          "type": "custom",
          "filter": [
            "lowercase"
          ]
        }
      },
      "analyzer": {
        "autocomplete": {
          "type": "custom",
          "tokenizer": "standard",
          "filter": [
            "lowercase",
            "autocomplete_filter"
          ]
        }
      }
    }
  },
  "mappings": {
    "properties": {
      "@context": {
        "type": "keyword"
      },
      "id": {
        "type": "keyword",
        "normalizer": "useLowercase",
        "fields": {
          "autocomplete": {
            "type": "text",
            "analyzer": "autocomplete",
            "search_analyzer": "standard"
          }
        }
      },
      "@type": {
        "type": "keyword"
      },
      "eventDate": {
        "type": "date"
      },
      "eventHandler": {
        "type": "keyword",
        "ignore_above": 200,
        "normalizer": "useLowercase",
        "fields": {
          "autocomplete": {
            "type": "text",
            "analyzer": "autocomplete",
            "search_analyzer": "standard"
          }
        }
      },
      "eventType": {
        "type": "keyword",
        "ignore_above": 200
      },
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "keyword",
            "normalizer": "useLowercase",
            "fields": {
              "autocomplete": {
                "type": "text",
                "analyzer": "autocomplete",
                "search_analyzer": "standard"
              }
            }
          },
          "@type": {
            "type": "keyword"
          },
          "@context": {
            "type": "keyword"
          },
          "collectionID": {
            "type": "keyword",
            "ignore_above": 200,
            "normalizer": "useLowercase",
            "fields": {
              "autocomplete": {
                "type": "text",
                "analyzer": "autocomplete",
                "search_analyzer": "standard"
              }
            }
          },
          "itemCount": {
            "type": "integer"
          },
          "itemID": {
            "type": "keyword",
            "ignore_above": 200,
            "normalizer": "useLowercase",
            "fields": {
              "autocomplete": {
                "type": "text",
                "analyzer": "autocomplete",
                "search_analyzer": "standard"
              }
            }
          },
          "itemLabel": {
            "type": "keyword",
            "ignore_above": 200,
            "normalizer": "useLowercase",
            "fields": {
              "autocomplete": {
                "type": "text",
                "analyzer": "autocomplete",
                "search_analyzer": "standard"
              }
            }
          },
          "itemType": {
            "type": "keyword",
            "ignore_above": 200
          },
          "notes": {
            "type": "keyword",
            "ignore_above": 200,
            "normalizer": "useLowercase",
            "fields": {
              "autocomplete": {
                "type": "text",
                "analyzer": "autocomplete",
                "search_analyzer": "standard"
              }
            }
          },
          "status": {
            "type": "keyword",
            "ignore_above": 200
          }
        }
      },
      "notes": {
        "type": "keyword",
        "ignore_above": 200,
        "normalizer": "useLowercase",
        "fields": {
          "autocomplete": {
            "type": "text",
            "analyzer": "autocomplete",
            "search_analyzer": "standard"
          }
        }
      },
      "numberOfPackages": {
        "type": "integer"
      },
      "sentBy": {
        "type": "keyword",
        "ignore_above": 200
      },
      "_meta": {
        "properties": {
          "created": {
            "type": "date"
          },
          "edited": {
            "type": "date"
          }
        }
      }
    }
  }
}