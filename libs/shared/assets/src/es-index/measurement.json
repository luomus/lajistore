{
  "settings": {
    "number_of_shards": 3,
    "index.mapping.ignore_malformed": true,
    "index.max_result_window": 1000000,
    "index.mapping.total_fields.limit": 10000,
    "index.mapping.nested_objects.limit": 100000,
    "index.max_ngram_diff": 4,
    "analysis": {
      "tokenizer": {
        "autocomplete": {
          "type": "ngram",
          "min_gram": 1,
          "max_gram": 4
        }
      },
      "normalizer": {
        "useLowercase": {
          "type": "custom",
          "filter": [
            "lowercase"
          ]
        }
      },
      "analyzer": {
        "autocomplete": {
          "type": "custom",
          "tokenizer": "autocomplete",
          "filter": [
            "lowercase"
          ]
        },
        "autocomplete_search": {
          "tokenizer": "lowercase"
        }
      }
    }
  },
  "mappings": {
    "properties": {
      "@context": {
        "type": "keyword"
      },
      "id": {
        "type": "keyword",
        "normalizer": "useLowercase",
        "fields": {
          "autocomplete": {
            "type": "text",
            "analyzer": "autocomplete",
            "search_analyzer": "autocomplete_search"
          }
        }
      },
      "@type": {
        "type": "keyword"
      },
      "DNAConcentrationNgPerMicroliter": {
        "type": "float"
      },
      "DNARatioOfAbsorbance260And280": {
        "type": "float"
      },
      "DNAVolumeMicroliters": {
        "type": "float"
      },
      "beakMillimeters": {
        "type": "float"
      },
      "bodyCentimeters": {
        "type": "float"
      },
      "bodyMillimeters": {
        "type": "float"
      },
      "earLengthMillimeters": {
        "type": "float"
      },
      "embryoCount": {
        "type": "float"
      },
      "follicleDiameterMillimeters": {
        "type": "float"
      },
      "footLengthMillimeters": {
        "type": "float"
      },
      "forearmMillimeters": {
        "type": "float"
      },
      "gonadMillimeters": {
        "type": "float"
      },
      "isPartOf": {
        "type": "keyword",
        "ignore_above": 200
      },
      "tailCentimeters": {
        "type": "float"
      },
      "tailMillimeters": {
        "type": "float"
      },
      "tarsusLengthMillimeters": {
        "type": "float"
      },
      "totalLengthCentimeters": {
        "type": "float"
      },
      "totalLengthMillimeters": {
        "type": "float"
      },
      "uterineScarCount": {
        "type": "float"
      },
      "weightGrams": {
        "type": "float"
      },
      "weightKilograms": {
        "type": "float"
      },
      "wingMaxMillimeters": {
        "type": "float"
      },
      "wingMillimeters": {
        "type": "float"
      },
      "wingMinMillimeters": {
        "type": "float"
      },
      "_meta": {
        "properties": {
          "created": {
            "type": "date"
          },
          "edited": {
            "type": "date"
          }
        }
      }
    }
  }
}