{
  "$schema": "http://json-schema.org/schema#",
  "type": "object",
  "subject": "PUU.event",
  "additionalProperties": false,
  "title": "",
  "description": "",
  "properties": {
    "@context": {
      "type": "string",
      "range": "xsd:string",
      "subject": "@context",
      "title": "Context for the PUU.event"
    },
    "id": {
      "type": "string",
      "range": "xsd:string",
      "subject": "id",
      "title": "Id for the PUU.event"
    },
    "@type": {
      "type": "string",
      "range": "xsd:string",
      "subject": "@type",
      "title": "Type for the PUU.event"
    },
    "agent": {
      "type": "string",
      "range": "MA.person",
      "subject": "PUU.agent",
      "title": "Doer"
    },
    "amount": {
      "type": "string",
      "range": "xsd:string",
      "subject": "PUU.amount",
      "description": "Enter the amount in the branch as n, addition to the previously given amount as +n or reduction as -n",
      "title": "Amount"
    },
    "amountEstimationUnit": {
      "type": "string",
      "range": "xsd:string",
      "subject": "PUU.amountEstimationUnit",
      "title": "Unit"
    },
    "branchID": {
      "type": "string",
      "range": "PUU.branch",
      "subject": "PUU.branchID",
      "description": "ID for the accossiated branch",
      "title": "Branch ID"
    },
    "date": {
      "type": "string",
      "format": "date",
      "range": "xsd:date",
      "subject": "PUU.date",
      "title": "Date"
    },
    "eRH": {
      "type": "string",
      "range": "xsd:string",
      "subject": "PUU.eRH",
      "title": "eRH"
    },
    "estimatedSeedQuantity": {
      "type": "integer",
      "range": "xsd:integer",
      "subject": "PUU.estimatedSeedQuantity",
      "title": "Quantity"
    },
    "eventType": {
      "type": "string",
      "enum": [
        "MY.eventTypeDiscarded",
        "MY.eventTypeDonating",
        "MY.eventTypeInventory",
        "MY.eventTypeMarkedDead",
        "MY.eventTypeOccasionalRemark",
        "MY.eventTypeTemporaryPlacement",
        "MY.eventTypePlanting",
        "MY.eventTypePlantedBackToWild",
        "MY.eventTypePollination",
        "MY.eventTypePrickingOut",
        "MY.eventTypePotting",
        "MY.eventTypePropagation",
        "MY.eventTypePropagationMaterialCollected",
        "MY.eventTypePruning",
        "MY.eventTypeSowing",
        "MY.eventTypeStoring",
        "MY.eventTypeTransferring",
        "MY.eventTypeOther",
        "MY.eventTypeAtlantis",
        "MY.eventTypeCleaning",
        "MY.eventTypeQuantityEstimation",
        "MY.eventTypeCutTesting",
        "MY.eventTypeDrying",
        "MY.eventTypeGerminationTesting",
        "MY.eventTypeGerminationRetesting",
        "MY.eventTypeGerminationControlling",
        "MY.eventTypeTZTesting",
        "MY.eventTypeSeedExchange"
      ],
      "enumNames": [
        "Discarded",
        "Donated or sold",
        "Inventoried",
        "Marked as dead",
        "Occasional remark",
        "Placed temporarily",
        "Planted or relocated",
        "Planted back to the wild",
        "Pollinated",
        "Pricked out",
        "Potted",
        "Vegetatively propagated",
        "Propagation material collected",
        "Pruned",
        "Sown",
        "Stored",
        "Transferred",
        "Other",
        "Atlantis",
        "Cleaned",
        "Quantity estimated",
        "Cut-tested",
        "Dried",
        "Germination tested (initial)",
        "Germination retested",
        "Germination controlled",
        "TZ-tested",
        "Seeds exchanged"
      ],
      "range": "MY.eventType",
      "subject": "PUU.eventType",
      "title": "Event type"
    },
    "germinationConditions": {
      "type": "string",
      "range": "xsd:string",
      "subject": "PUU.germinationConditions",
      "title": "Germination conditions"
    },
    "germinationPercentage": {
      "type": "integer",
      "range": "xsd:integer",
      "subject": "PUU.germinationPercentage",
      "title": "Germination percentage"
    },
    "germinationRate": {
      "type": "number",
      "range": "xsd:decimal",
      "subject": "PUU.germinationRate",
      "title": "Germination rate"
    },
    "lifeStage": {
      "type": "string",
      "enum": [
        "",
        "MY.plantLifeStageSterile",
        "MY.plantLifeStageFertile",
        "MY.plantLifeStageSeed",
        "MY.plantLifeStageSprout",
        "MY.plantLifeStageBud",
        "MY.plantLifeStageFlower",
        "MY.plantLifeStageWitheredFlower",
        "MY.plantLifeStageRipeningFruit",
        "MY.plantLifeStageRipeFruit",
        "MY.plantLifeStageDeadSprout",
        "MY.plantLifeStageSubterranean"
      ],
      "enumNames": [
        "",
        "sterile",
        "fertile",
        "seed / spore / fruit",
        "sprout",
        "bud",
        "flower",
        "withered flower",
        "ripening seed / fruit / spore",
        "ripe seed / fruit / spore",
        "dead sprout",
        "subterranean"
      ],
      "range": "MY.plantLifeStageEnum",
      "subject": "PUU.lifeStage",
      "title": "Life stage"
    },
    "notes": {
      "type": "string",
      "range": "xsd:string",
      "subject": "PUU.notes",
      "title": "Notes"
    },
    "numberOfSeedsEmpty": {
      "type": "integer",
      "range": "xsd:integer",
      "subject": "PUU.numberOfSeedsEmpty",
      "title": "Number seeds empty"
    },
    "numberOfSeedsFull": {
      "type": "integer",
      "range": "xsd:integer",
      "subject": "PUU.numberOfSeedsFull",
      "title": "Number seed full"
    },
    "numberOfSeedsInfested": {
      "type": "integer",
      "range": "xsd:integer",
      "subject": "PUU.numberOfSeedsInfested",
      "title": "Number seed infested"
    },
    "numberOfSeedsMouldy": {
      "type": "integer",
      "range": "xsd:integer",
      "subject": "PUU.numberOfSeedsMouldy",
      "title": "Number seed mouldy"
    },
    "numberOfSeedsPartFull": {
      "type": "integer",
      "range": "xsd:integer",
      "subject": "PUU.numberOfSeedsPartFull",
      "title": "Number seed part-full"
    },
    "numberOfSeedsTested": {
      "type": "integer",
      "range": "xsd:integer",
      "subject": "PUU.numberOfSeedsTested",
      "title": "Number seed tested"
    },
    "percentageSeedsFull": {
      "type": "integer",
      "range": "xsd:integer",
      "subject": "PUU.percentageSeedsFull",
      "title": "Test result (%)"
    },
    "seedsExchangedInstitution": {
      "type": "string",
      "range": "MOS.organization",
      "subject": "PUU.seedsExchangedInstitution",
      "title": "Institution"
    },
    "testPassed": {
      "type": "boolean",
      "range": "xsd:boolean",
      "subject": "PUU.testPassed",
      "title": "Test passed"
    },
    "viabilityPercentage": {
      "type": "integer",
      "range": "xsd:integer",
      "subject": "PUU.viabilityPercentage",
      "title": "Viability percentage"
    }
  },
  "required": [
    "branchID",
    "eventType"
  ]
}