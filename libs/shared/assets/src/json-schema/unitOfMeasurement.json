{
  "$schema": "http://json-schema.org/schema#",
  "type": "object",
  "subject": "TDF.unitOfMeasurement",
  "additionalProperties": false,
  "title": "Unit of measurement",
  "description": "",
  "properties": {
    "@context": {
      "type": "string",
      "range": "xsd:string",
      "subject": "@context",
      "title": "Context for the Unit of measurement"
    },
    "id": {
      "type": "string",
      "range": "xsd:string",
      "subject": "id",
      "title": "Id for the Unit of measurement"
    },
    "@type": {
      "type": "string",
      "range": "xsd:string",
      "subject": "@type",
      "title": "Type for the Unit of measurement"
    },
    "baseConversion": {
      "type": "number",
      "range": "xsd:decimal",
      "subject": "TDF.baseConversion",
      "title": "Conversion factor"
    },
    "baseUnit": {
      "type": "string",
      "range": "TDF.unitOfMeasurement",
      "subject": "TDF.baseUnit",
      "title": "Base unit of measurement"
    },
    "isBaseUnit": {
      "type": "boolean",
      "range": "xsd:boolean",
      "subject": "TDF.isBaseUnit",
      "title": "Is base unit?"
    },
    "rdfs:label": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "en": {
          "type": "array",
          "uniqueItems": false,
          "items": {
            "type": "string"
          },
          "minItems": 0
        },
        "fi": {
          "type": "array",
          "uniqueItems": false,
          "items": {
            "type": "string"
          },
          "minItems": 0
        },
        "sv": {
          "type": "array",
          "uniqueItems": false,
          "items": {
            "type": "string"
          },
          "minItems": 0
        }
      },
      "range": "xsd:string",
      "subject": "rdfs:label",
      "title": "label"
    }
  },
  "required": [
    "baseConversion",
    "isBaseUnit"
  ]
}