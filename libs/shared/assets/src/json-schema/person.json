{
  "$schema": "http://json-schema.org/schema#",
  "type": "object",
  "subject": "MA.person",
  "additionalProperties": false,
  "title": "Person",
  "description": "",
  "properties": {
    "@context": {
      "type": "string",
      "range": "xsd:string",
      "subject": "@context",
      "title": "Context for the Person"
    },
    "id": {
      "type": "string",
      "range": "xsd:string",
      "subject": "id",
      "title": "Id for the Person"
    },
    "@type": {
      "type": "string",
      "range": "xsd:string",
      "subject": "@type",
      "title": "Type for the Person"
    },
    "LTKMLoginName": {
      "type": "string",
      "range": "xsd:string",
      "subject": "MA.LTKMLoginName",
      "title": "LTKM-käyttäjätunnus"
    },
    "address": {
      "type": "string",
      "range": "xsd:string",
      "subject": "MA.address",
      "title": "Address"
    },
    "defaultLanguage": {
      "type": "string",
      "range": "xsd:string",
      "subject": "MA.defaultLanguage",
      "title": "Default language"
    },
    "defaultQNamePrefix": {
      "type": "string",
      "range": "xsd:string",
      "subject": "MA.defaultQNamePrefix",
      "title": "Default QName prefix"
    },
    "description": {
      "type": "string",
      "range": "xsd:string",
      "subject": "MA.description",
      "title": "Kuvaus"
    },
    "emailAddress": {
      "type": "string",
      "range": "xsd:string",
      "subject": "MA.emailAddress",
      "title": "Sähköposti"
    },
    "facebookLoginName": {
      "type": "string",
      "range": "xsd:string",
      "subject": "MA.facebookLoginName",
      "title": "Facebook-tunnus"
    },
    "fieldjournalLoginName": {
      "type": "array",
      "uniqueItems": false,
      "items": {
        "type": "string"
      },
      "minItems": 0,
      "range": "xsd:string",
      "subject": "MA.fieldjournalLoginName",
      "title": "Hatikka userid (new)"
    },
    "firstJoined": {
      "type": "string",
      "format": "date",
      "range": "xsd:date",
      "subject": "MA.firstJoined",
      "title": "Ensimmäinen sisäänkirjautuminen"
    },
    "fullName": {
      "type": "string",
      "range": "xsd:string",
      "subject": "MA.fullName",
      "title": "Koko nimi"
    },
    "givenNames": {
      "type": "string",
      "range": "xsd:string",
      "subject": "MA.givenNames",
      "title": "Etunimet"
    },
    "googleLoginName": {
      "type": "string",
      "range": "xsd:string",
      "subject": "MA.googleLoginName",
      "title": "Google login name"
    },
    "group": {
      "type": "string",
      "range": "xsd:string",
      "subject": "MA.group",
      "description": "Group, membership or any other way that allows the user to distinguish themselves from people with the same name",
      "title": "Group"
    },
    "hakaLoginName": {
      "type": "string",
      "range": "xsd:string",
      "subject": "MA.hakaLoginName",
      "title": "Haka-tunnus"
    },
    "hatikkaLoginName": {
      "type": "array",
      "uniqueItems": false,
      "items": {
        "type": "string"
      },
      "minItems": 0,
      "range": "xsd:string",
      "subject": "MA.hatikkaLoginName",
      "title": "OLD Hatikka login name"
    },
    "inaturalistLoginName": {
      "type": "string",
      "range": "xsd:string",
      "subject": "MA.inaturalistLoginName",
      "title": "Inaturalist-tunnus"
    },
    "inheritedName": {
      "type": "string",
      "range": "xsd:string",
      "subject": "MA.inheritedName",
      "title": "Sukunimi"
    },
    "insectDatabaseLoginName": {
      "type": "array",
      "uniqueItems": false,
      "items": {
        "type": "string"
      },
      "minItems": 0,
      "range": "xsd:string",
      "subject": "MA.insectDatabaseLoginName",
      "title": "insectDatabaseLoginName"
    },
    "lajiAuthLoginName": {
      "type": "string",
      "range": "xsd:string",
      "subject": "MA.lajiAuthLoginName",
      "title": "Laji.fi -tunnus"
    },
    "lintuvaaraLoginName": {
      "type": "array",
      "uniqueItems": false,
      "items": {
        "type": "string"
      },
      "minItems": 0,
      "range": "xsd:string",
      "subject": "MA.lintuvaaraLoginName",
      "title": "lintuvaaraLoginName"
    },
    "omariistaLoginName": {
      "type": "string",
      "range": "xsd:string",
      "subject": "MA.omariistaLoginName",
      "title": "Omariista-tunnus"
    },
    "organisation": {
      "type": "array",
      "uniqueItems": false,
      "items": {
        "type": "string"
      },
      "minItems": 0,
      "range": "MOS.organization",
      "subject": "MA.organisation",
      "title": "Organisaatio"
    },
    "preferredName": {
      "type": "string",
      "range": "xsd:string",
      "subject": "MA.preferredName",
      "title": "Kutsumanimi"
    },
    "previousEmailAddress": {
      "type": "array",
      "uniqueItems": false,
      "items": {
        "type": "string"
      },
      "minItems": 0,
      "range": "xsd:string",
      "subject": "MA.previousEmailAddress",
      "title": "Previously used e-mail address"
    },
    "role": {
      "type": "array",
      "uniqueItems": false,
      "items": {
        "type": "string",
        "enum": [
          "",
          "MA.admin",
          "MA.securePortalUser",
          "MA.sensitiveInformationApprovalRequestHandler",
          "MA.taxonEditorUser",
          "MA.taxonEditorUserDescriptionWriterOnly",
          "MA.haukkaUser",
          "MA.satelliteDataManagementUser",
          "MA.invasivePortalAdmin",
          "MA.luomusSpaceCalendarUser",
          "MA.luomusSpaceOpeningTimesUser"
        ],
        "enumNames": [
          "",
          "Admin (ICT-team)",
          "Viranomaiset.laji.fi user",
          "Sensitive information administrator",
          "Taxoneditor.laji.fi user",
          "Species description writer",
          "Haukka bird station app user",
          "Satelliitti.laji.fi user",
          "Invasive species portal administrator",
          "Luomus space reservation calendar",
          "Luomuksen yleisökohteiden aukioloajat"
        ]
      },
      "minItems": 0,
      "range": "MA.roles",
      "subject": "MA.role",
      "title": "Rooli"
    },
    "roleAnnotation": {
      "type": "string",
      "enum": [
        "",
        "MMAN.expert",
        "MMAN.basic",
        "MMAN.owner",
        "MMAN.formAdmin",
        "MMAN.ictAdmin"
      ],
      "enumNames": [
        "",
        "expert",
        "basic",
        "owner",
        "Form admin",
        "ICT admin"
      ],
      "range": "MMAN.requiredRolesEnum",
      "subject": "MA.roleAnnotation",
      "title": "Available annotation roles"
    },
    "roleKotka": {
      "type": "string",
      "enum": [
        "",
        "MA.admin",
        "MA.advanced",
        "MA.member",
        "MA.guest"
      ],
      "enumNames": [
        "",
        "Admin (ICT-team)",
        "advanced",
        "member",
        "guest"
      ],
      "range": "MA.roleKotkaEnum",
      "subject": "MA.roleKotka",
      "title": "User role in Kotka"
    },
    "virtuLoginName": {
      "type": "string",
      "range": "xsd:string",
      "subject": "MA.virtuLoginName",
      "title": "Virtu-tunnus"
    },
    "yearOfBirth": {
      "type": "string",
      "range": "xsd:gYear",
      "subject": "MA.yearOfBirth",
      "title": "Year of birth"
    }
  },
  "required": [
    "emailAddress",
    "fullName"
  ]
}