{
  "$schema": "http://json-schema.org/schema#",
  "type": "object",
  "subject": "MPO.news",
  "additionalProperties": false,
  "title": "News",
  "description": "",
  "properties": {
    "@context": {
      "type": "string",
      "range": "xsd:string",
      "subject": "@context",
      "title": "Context for the News"
    },
    "id": {
      "type": "string",
      "range": "xsd:string",
      "subject": "id",
      "title": "Id for the News"
    },
    "@type": {
      "type": "string",
      "range": "xsd:string",
      "subject": "@type",
      "title": "Type for the News"
    },
    "content": {
      "type": "string",
      "range": "xsd:string",
      "subject": "MPO.content",
      "title": "content"
    },
    "eventDate": {
      "type": "string",
      "format": "date-time",
      "range": "xsd:dateTime",
      "subject": "MPO.eventDate",
      "title": "Date and time to for the news event"
    },
    "language": {
      "type": "string",
      "enum": [
        "en",
        "fi",
        "sv"
      ],
      "enumNames": [
        "english",
        "finnish",
        "swedish"
      ],
      "range": "MZ.languageEnum",
      "subject": "MPO.language",
      "title": "language"
    },
    "publishDate": {
      "type": "string",
      "format": "date-time",
      "range": "xsd:dateTime",
      "subject": "MPO.publishDate",
      "title": "publish date"
    },
    "targetIndividualIds": {
      "type": "array",
      "uniqueItems": false,
      "items": {
        "type": "string"
      },
      "minItems": 0,
      "range": "MXA.individual",
      "subject": "MPO.targetIndividualIds",
      "title": "Target individuals"
    },
    "targets": {
      "type": "array",
      "uniqueItems": false,
      "items": {
        "type": "string"
      },
      "minItems": 0,
      "range": "xsd:anyURI",
      "subject": "MPO.targets",
      "title": "Targets for the news"
    },
    "title": {
      "type": "string",
      "range": "xsd:string",
      "subject": "MPO.title",
      "title": "title"
    }
  },
  "required": [
    "content",
    "eventDate",
    "language",
    "title"
  ]
}