{
  "name": "@luomus/laji-schema",
  "version": "2.0.81",
  "description": "Common autogenerated Typescript interfaces and classes for the schemas used by LUOMUS.",
  "license": "MIT",
  "scripts": {
    "tsc": "npx tsc -b tsconfig.cjs.json tsconfig.esm.json tsconfig.types.json",
    "gen-dto": "mkdir -p dist/dto && cp src/classes/*.ts dist/dto/ && cd dist/dto && for f in *.ts; do mv -- \"$f\" \"${f%.ts}.dto.ts\"; done && sed -i \"s/'$/.dto'/g\" index.dto.ts && sed -i \"s/'\\.\\/'/'\\.\\/index.dto'/gm\" *.dto.ts && cd ../..",
    "prepublishOnly": "npm run tsc && npm run gen-dto",
    "postpublish": "rm -rf dist"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "https://github.com/luomus/lajistore.git",
    "directory": "libs/shared/schemas"
  },
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js"
    },
    "./classes": {
      "types": "./dist/types/classes/index.d.ts",
      "require": "./dist/cjs/classes/index.js",
      "import": "./dist/esm/classes/index.js"
    },
    "./models": {
      "types": "./dist/types/models/index.d.ts",
      "require": "./dist/cjs/models/index.js",
      "import": "./dist/esm/models/index.js"
    },
    "./dto": {
      "types": "./dist/dto/index.dto.ts",
      "import": "./dist/dto/index.dto.ts"
    }
  },
  "typesVersions": {
    "*": {
      "classes": [
        "./dist/types/classes/index.d.ts"
      ],
      "models": [
        "./dist/types/models/index.d.ts"
      ],
      "dto": [
        "./dist/dto/index.dto.ts"
      ],
      "*": [
        "./dist/types/index.d.ts"
      ]
    }
  },
  "devDependencies": {
    "typescript": "~4.1.5"
  }
}
